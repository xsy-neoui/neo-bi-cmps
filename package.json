{
  "name": "neo-bi-cmps",
  "version": "1.0.6",
  "description": "neo BI 自定义组件（react&ts技术栈）",
  "framework": "react-ts",
  "keywords": [
    "react&ts技术栈",
    "neo自定义组件"
  ],
  "author": "wibetter",
  "license": "MIT",
  "scripts": {
    "preview": "neo preview",
    "linkDebug": "neo linkDebug",
    "neoLogin": "neo login",
    "pushCmp": "neo push cmp",
    "pullCmp": "neo pull cmp",
    "deleteCmp": "neo delete cmp",
    "build2lib": "neo build2lib",
    "format": "prettier --write \"src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}\""
  },
  "files": [
    "lib/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/**/*.{js,jsx,ts,tsx,vue,scss,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "neo-register": "^1.1.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "axios": "^0.27.2",
    "antd": "^4.9.4",
    "lodash": "^4.17.21",
    "neo-open-api": "^1.1.9",
    "@wibetter/json-editor": "^6.0.5",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.26.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.15",
    "@types/axios": "^0.14.0",
    "neo-cmp-cli": "^1.10.13",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 8.0.0"
  }
}
